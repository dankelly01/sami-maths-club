{
  "hosting": {
    "public": "dist/sami-maths-club-app",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "function": "seoHost"
      }
    ]
  },
  "functions": {
    "predeploy": [
      "yarn --cwd \"$RESOURCE_DIR\" install",
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "source": "functions"
  }
}
